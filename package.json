{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "NODE_ENV=development npm run build && node dist/main.js",
    "deploy:commands": "npm run build && NODE_ENV=development node dist/deploy-commands.js",
    "lint": "prettier --write src && eslint --fix src",
    "build": "tsc",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript"
    ],
    "env": {
      "browser": false,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": [
        "tsconfig.json"
      ]
    },
    "rules": {
      "no-console": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "wall-e",
  "author": "Pedro",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "^0.8.0",
    "discord-api-types": "^0.27.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "libsodium-wrappers": "^0.7.9",
    "opusscript": "^0.0.8"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix src",
    "*.{ts,css,md}": "prettier --write src"
  }
}
